#: pg_manual_payment.module:43;55
msgid "Payment Notification from !site"
msgstr "Уведомление о платеже от !site"

#: pg_manual_payment.module:44
msgid ""
"Dear !username\n"
"Your Payment #!txnid will be checked today.\n"
"\n"
"Amount: !price\n"
"Payment Account: !purse\n"
"\n"
"Best regards,\n"
"!site\n"
msgstr ""
"Уважаемый !username\n"
"Ваш Оплата #!txnid будут проверены сегодня.\n"
"\n"
"Сумма: !price\n"
"Оплата счета: !purse\n"
"\n"
"С уважением,\n"
"!site\n"

#: pg_manual_payment.module:56
msgid ""
"Dear Admin\n"
"User !username just sent a payment #!txnid\n"
"\n"
"Amount: !price\n"
"Payment Account: !purse\n"
"\n"
"Please check it out and process it.\n"
"\n"
"Best regards,\n"
"!site\n"
msgstr ""
"Уважаемый Админ\n"
"Пользователь !username отправили платеж #!txnid\n"
"\n"
"Сумма: !price\n"
"Оплата счета: !purse\n"
"Пожалуйста, проверьте его и обработайте.\n"
"\n"
"С уважением,\n"
"!site\n"

